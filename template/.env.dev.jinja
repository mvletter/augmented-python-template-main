# !!! DISCLAIMER !!! DO NOT EDIT THIS FILE
{% if include_database %}
POSTGRES_DSN=postgresql://{{ service_name }}:dbpassword@{{ service_name }}-psql:5432/{{ service_name }}-local
{% endif %}
{% if include_redis %}
REDIS_DSN=redis://{{ service_name }}-redis:6379
{% endif %}
{% if use_nats %}
NATS_SERVER_URL=nats://nats:4222
NATS_CONSUMER_NAME="{{ service_name }}"
{% endif  %}
{% if use_resgate %}
ENABLE_RESGATE=1
{% endif  %}
SENTRY_DSN=

LOG_JSON=0

TERM

PYTHONDONTWRITEBYTECODE=1
PYTHONUNBUFFERED=1

TRACING=0
REMOTE_TRACING=1
OTLP_ENDPOINT=http://otel-collector:4318/v1/traces
